#include<iostream>
using namespace std;

int main(){
	char c;
    int i=0,j=0,k=0;
    while(c!='$')
    {
    	 c=cin.get();
        if(c>=97&& c<=122)
        	i++;
        else if(c>=48&& c<=57)
            j++;
        else if(c==' '||c =='\t'||c=='\n')
            k++;
    }
    cout<<i<<" "<<j<<" "<<k;   
}


